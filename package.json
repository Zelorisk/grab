{
  "name": "ip-tracker",
  "version": "1.0.0",
  "description": "IP tracking and geolocation application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "setup": "bash setup-ngrok.sh",
    "ngrok": "npx ngrok http 3001"
  },
  "keywords": ["ip-tracker", "geolocation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "nanoid": "^3.3.7",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
